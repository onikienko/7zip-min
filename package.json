{
  "name": "7zip-min",
  "version": "3.0.0",
  "description": "Standalone cross-platform zip/unzip with 7za",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./src/index.js"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "release": "dotenv release-it --",
    "preview": "release-it --dry-run",
    "test": "ava test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onikienko/7zip-min.git"
  },
  "files": [
    "src/index.js",
    "src/index.mjs",
    "src/index.d.ts"
  ],
  "keywords": [
    "7z",
    "7zip",
    "7-zip",
    "7za",
    "zip",
    "compress",
    "decompress",
    "pack",
    "extract",
    "unzip",
    "unpack",
    "lzma",
    "gzip",
    "bzip2",
    "tar"
  ],
  "author": "Mykhailo Onikiienko <michael.onikienko@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onikienko/7zip-min/issues"
  },
  "homepage": "https://github.com/onikienko/7zip-min#readme",
  "dependencies": {
    "7zip-bin": "5.1.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.5",
    "@types/fs-extra": "^11.0.4",
    "ava": "^6.4.1",
    "dotenv-cli": "^11.0.0",
    "fs-extra": "^11.3.3",
    "fsify": "^5.0.0",
    "prettier": "^3.8.1",
    "release-it": "^19.2.4"
  }
}
